{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", 4],
        "quotes": ["warn", "double"],
        "semi": ["warn", "always"],
        "no-unused-vars": ["warn"]
    }
}
